{
  "name": "react-typescript-cssmodules-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "immutable": "^4.0.0-rc.9",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "typed-intl": "^1.0.0",
    "typed-redux-actions": "^1.1.0"
  },
  "scripts": {
    "css-types": "tcm -p src/**/*.css -c -o generated",
    "build": "yarn run css-types && webpack -p --config=config/webpack/prod.js",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "yarn run start-dev",
    "start-dev": "yarn run css-types && webpack-dev-server --config=config/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --env=jsdom",
    "test-coverage": "jest --coverage --env=jsdom"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.12",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/intl": "^1.2.0",
    "@types/intl-messageformat": "^1.3.0",
    "@types/jest": "^21.1.4",
    "@types/node": "^8.0.46",
    "@types/react": "^16.0.16",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.0.8",
    "awesome-typescript-loader": "^3.2.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jest": "^21.3.0-beta.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^4.0.1",
    "react-hot-loader": "^3.1.1",
    "style-loader": "0.19.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typed-css-modules-loader": "^0.0.11",
    "typescript": "~2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  },
  "resolutions": {
    "@types/react": "16.0.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "/src/.*\\.test\\.(ts|tsx)$",
    "moduleDirectories": [
      "src",
      "generated/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "\\\\.d\\\\.ts$",
      "\\\\.msg\\\\.ts$"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
